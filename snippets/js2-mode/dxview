define([
	'backbone'
], function(Backbone) {

	return Backbone.View.extend({

		initialize: function() {
			Backbone.View.prototype.initialize.apply(this, arguments);
		},

		render: function() {
			$0
			return this;
		},

		remove: function() {
			Backbone.View.prototype.remove.apply(this, arguments);
		}

	});

});